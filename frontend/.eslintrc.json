{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "prettier"
    ],
    "plugins": ["react", "prettier"],
    "rules": {
      "prettier/prettier": ["error"],
      "react/prop-types": "off",
      "no-unused-vars": "warn"
    }
  }
  